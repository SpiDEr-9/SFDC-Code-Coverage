{
   "author": "Bhushan Mhatre",
   "background": {
      "persistent": false,
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      "default_popup": "view/popup.html"
   },
   "content_scripts": [ {
      "js": [ "js/jquery.js", "js/forcetk.js", "js/tinycon.js", "js/contentscript.js" ],
      "matches": [ "https://*.salesforce.com/*", "https://*.force.com/*" ]
   } ],
   "description": "Shows the code coverage and code covered by each method induvidually",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon116.png",
      "32": "icons/icon132.png",
      "48": "icons/icon148.png"
   },
   "manifest_version": 3,
   "name": "SF Code Coverage Viewer",
   "permissions": [ "storage" ],
   "version": "1.0.0"
}
